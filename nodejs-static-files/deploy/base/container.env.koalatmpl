# Environment variables to be passed into containers
FOLDER={{ inputs.folder }}
DEFAULT_EXT={{ inputs.ext }}
PORT={{ inputs.port }}
SHOW_DIRECTORY_LISTINGS={{ inputs.showDirectoryListings }}
NO_DOTFILES={{ inputs.noDotfiles }}
GZIP={{ inputs.gzip }}
BROTLI={{ inputs.brotli }}
MIMETYPES={{ inputs.mimetypes }}
CACHE_TIME={{ inputs.cacheTime }}
PROXY={{ inputs.proxy }}
PROXY_OPTIONS={{ inputs.proxyOptions }}
CORS={{ inputs.cors }}
SILENT={{ inputs.silent }}